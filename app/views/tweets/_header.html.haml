.site-header
  .site-header-blank-box
  = link_to '',root_path,class:'site-header-logo'
  = form_with url: search_tweets_path, method: :get,class:'search-form' ,local: true do |form| 
    = form.text_field :keyword,placeholder: "検索ワードを入力", type: "search",class:'form-input-box'
    %button{type:'submit',class:'form-submit-btn'}
      %i{class:'fa fa-search'}