-if tweet.user_id != current_user.id
  %span{class:"like"}
    -if tweet.iine?(current_user)
      =form_for(tweet.likes.find_by(user_id: current_user.id), method: :delete, remote: true) do |f| 
        = button_tag(class: "btn btn-default btn-xs") do 
          = content_tag :span,'#', class: "glyphicon glyphicon-heart"
    -else
      = form_for(tweet.likes.build, remote: true) do |f| 
        = hidden_field_tag :tweet_id, tweet.id 
        = button_tag(class: "btn btn-default btn-xs") do 
          = content_tag :span, "#", class: "glyphicon glyphicon-heart-empty" 

